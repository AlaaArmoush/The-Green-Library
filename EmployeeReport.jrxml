	<queryString language="SQL">
		<![CDATA[SELECT 
    employee.empfname || ' ' || employee.emplname AS Employee_name,
    COUNT(book.bookid) AS Total_books_added,
    STRING_AGG(book.bookname, CHR(10)) AS Added_books
FROM 
    employee
LEFT JOIN 
    book ON book.empid = employee.empid
GROUP BY 
    employee.empid, employee.empfname, employee.emplname;]]>
