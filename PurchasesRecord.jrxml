	<queryString language="SQL">
		<![CDATA[SELECT 
    customer.custfname || ' ' || customer.custlname AS Customer_name,
    COUNT(purchases.bookid) AS Total_books_purchased,
    STRING_AGG(book.bookname, CHR(10)) AS Purchased_books
FROM 
    customer
INNER JOIN 
    purchases ON purchases.custid = customer.custid
LEFT JOIN 
    book ON book.bookid = purchases.bookid
GROUP BY 
    customer.custid, customer.custfname, customer.custlname;
]]>
	</queryString>
