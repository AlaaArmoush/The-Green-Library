	<queryString language="SQL">
		<![CDATA[SELECT 
    publisher.pubname,
    COUNT(book.bookid) AS Total_published,
    STRING_AGG(book.bookname, CHR(10)) AS Published_books
FROM 
    publisher
LEFT JOIN 
    book ON book.pubid = publisher.pubid
GROUP BY 
    publisher.pubid, publisher.pubname;]]>
	</queryString>
